@model IList<Articles.Web.Models.ArticleViewModel>

@foreach (var item in Model)
{
    <div class="row" style="margin:20px">
        <div class="media" style="border:1px solid blue;height:160px; width:700px; background-color:white;">
            <a class="media-left col-md-3" href="#">
                @if (item.Images.Any(f => f.FileType == FileType.Avatar))
                {
                    <img src="~/File?id=@item.Images.First(f => f.FileType == FileType.Avatar).FileId" style="width:160px; height:160px" alt="avatar" />
                }
                else
                {
                    <img class="media-object" src="data:image/jpeg;base64," style="width:160px; height:160px; border:0px;" alt="image">
                }
                            </a>
            <div class="media-body col-md-9">
                <a class="media-heading" style="margin-top:15px; font:25px solid red"  href="Admin/Details/@item.Id">@item.Title</a>
                <p style="word-wrap:break-word; margin:10px">@item.Content</p>
            </div>
        </div>
    </div>
}
